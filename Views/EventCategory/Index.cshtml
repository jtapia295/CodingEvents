
@{
}

@model List<EventCategory>


<h1>Here are your events</h1>

@if (Model.Count == 0)
{
    <p>No Categories yet!</p>
}
else
{
<table class="table">
    <tr>
        <th>ID</th>
        <th>Category</th>

    </tr>
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.Id</td>
            <td>@category.Name</td>
        </tr>
    }
</table>
}